            </binding>
        </element>
        <element id="Goal.target.measure">
            <path value="Goal.target.measure"/>
            <binding>
                <strength value="required"/>
            </binding>
        </element>
        <element id="Goal.addresses">
            <path value="Goal.addresses"/>
            <slicing>
                <discriminator>
                    <type value="profile"/>
                    <path value="resolve()"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <type>
