                <targetProfile value="http://hl7.org/fhir/us/sdoh-cc/StructureDefinition/SDOHCC-Observation-ScreeningResponseBase-1"/>
            </type>
            <mustSupport value="true"/>
        </element>
        <element id="Goal.outcomeReference">
            <path value="Goal.outcomeReference"/>
            <slicing>
                <discriminator>
                    <type value="profile"/>
                    <path value="resolve()"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
        </element>
        <element id="Goal.outcomeReference:SupportedOutcomeReference">
